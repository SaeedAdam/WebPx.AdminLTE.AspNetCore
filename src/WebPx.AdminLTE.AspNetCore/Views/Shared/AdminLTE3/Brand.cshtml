@inject IAdminLTE adminLTE
@{
    var options = @adminLTE.Options;
}
<a asp-page="/Index" asp-area="" class="brand-link">
    <img src="@options.SiteLogo" alt="AdminLTE Logo" class="brand-image img-circle elevation-3" style="opacity: .8">
    <span class="brand-text font-weight-light">@options.SiteName</span>
</a>
